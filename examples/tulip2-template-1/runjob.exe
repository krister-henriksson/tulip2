#! /bin/bash



for fm in CG GN LM DL PM SM DE PS BC GS SA ; do

    cp specsfile.in.base specsfile.in
    echo "pot:fitmet       = $fm    options: CG, GN, LM, DL, PM, SM, DE, PS, BC, GS, or SA" >> specsfile.in

    valgrind tulip -pf potinfo.in -sf specsfile.in -gf geominfo.in >& out/tulip.out-${fm}-debug

    tulip -pf potinfo.in -gf geominfo.in -sf specsfile.in      >& out/tulip.out-${fm}-ser

    n=1
    while [ $n -le 4 ] ; do
	tulip -pf potinfo.in -gf geominfo.in -sf specsfile.in -omp_nt $n >& out/tulip.out-${fm}-omp-n$n
	n=$((n+1))
    done

done



